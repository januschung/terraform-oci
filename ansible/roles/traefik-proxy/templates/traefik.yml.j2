log:
  level: INFO

accessLog: {}

api:
  dashboard: true

entryPoints:
  web:
    address: ":80"
  websecure:
    address: ":443"

providers:
  docker: {}

certificatesResolvers:
  letsencrypt:
    acme:
      email: "{{ acme_email }}"
      storage: acme.json
      httpChallenge:
        entryPoint: web
